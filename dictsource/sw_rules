        h        h

.group i
        i        i
      
.group j
        j        J
	    j (w     J

.group k
        k        k
     K) k (@@    _|k
        kh       x

.group l
        l        l


.group m
        m        m
     _) m        m-
        m (A     m
     K) m (@@    _|m
     K) m (C     m-
     K) m (b@@   _|m
     K) m (v@   _|m
     K) m (w@@   _|m

.group n
        n        n
     _) n        n-
        n (A     n
        n (@@    n
        ny       n^
        n (ch    n-
        n (j     n^
        ng ('      N
        n (k     N
     _) n (k     N-
     _) n (k@@   N-
        n (g     N



.group o
        o        o
    

.group p
        p        p

.group q
        q        k

.group r
        r          R
     A) r (A       R
     C) r (A       @-*
        rr         RR

.group s
        s        s
        sh       S

.group t
        t        t
        th       T

.group u
        u        u
		
	

