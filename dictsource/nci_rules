// translation rules for Classical Nahuatl
// This file is UTF-8 encoded

.L01 e ē i ī    // letter group for front vowels


.group a
	a	a

.group ā
	ā	a:

.group c
	c	k
	c (L01	s
	ch	tS
	cu	kw

.group e
	e	e

.group ē
	ē	e:
	ē (_	'e:    // vocative, stress in final vowel

.group h
	h	?
	hu	w

.group i
	i	i

.group ī
	ī	i:

.group l
	l	l

.group m
	m	m


.group n
