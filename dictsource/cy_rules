// translation rules for Welsh language
// This file is UTF-8 encoded

.L01 ch dd ff ll th     // digraph consonants

.group a
     _)  a (_      A:
         a         A:
         a (C      a
         a (CC     a
         aa        A:

         a (b      A:
         a (d      A:
         a (g      A:
         a (ch     A:
         a (th     A:
         a (ff     A:
         a (s      A:
         a (f      A:
         a (dd     A:

         ae        AY
         ai        aI
         au        aY
//      @) au (_     a
         aw (+     aU


.group b
     _)  b (_      bi:
         b         b

.group c
     _)  c (_      Ek
     _)  ch (_     Ex
         c         k
         ch        x
     e)  ch        C
     i)  ch        C
     u)  ch        C

     _)  cwn       kUm


