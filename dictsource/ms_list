// punctuation
_!	t,anda||s'ER2u
_"	t,andak'utip
Â£	ponst'E*RliN
%	p@*Ratus
^	t,andat'opi
&	d'an
_*	b'intaN
_(	k,uR2uN||b'uk@
_)	k,uR2uN||t'utup
__	g,aR2is||b'awah
_-	t,andah'ubuN
+	c'ampor
=	s,am@||d'@Nan
_{	k,uR2uN||kuR2'awal
_}	k,uR2uN||kuR2,awal||t'utup
_[	k,uR2uN||s'iku
_]	k,uR2uN||s,iku||t'utup
_:	t,iti?||d'u@
_;	t,iti?||k'om@
@	a||k'EOng
_'	k,utip||t'uNgal
~	t'ilde
#	p'agaR
_<	k,uR2aN||d'aR2i
_>	l,Ebih||d'aR2i
_,	k'om@
_.	t'iti?
_?	t,and@||t'an^@
/	stR2'ok
_\	'bEkslES





