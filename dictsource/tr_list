// function words

// conjunctions
ve	$u $brk	// and
yoksa	$u $brk	// or
veya	$u $brk	// or
ama	$brk $1	// but
ki	$u $brk	// that

mı	$u $only	// question
mi	$u $only
mu	$u $only
mü	$u $only


// pronouns
bu	$u+ $only	// this
şu	$u+ $only	// that

ben	$u+ $only	// I
sen	$u+ $only	// you
o	$u+ $only	// it
siz	$u+ $only	// we
biz	$u+ $only	// you



bir	$u+ $only	// a (or one)

da	$u $only	// also
de	$u $only
ta	$u $only
te	$u $only

ile	$u $only


// EXCEPTIONS
merhaba		$1

